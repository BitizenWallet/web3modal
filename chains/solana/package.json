{
  "name": "@web3modal/solana",
  "version": "2.0.0-alpha.7",
  "description": "Solana connector for web3modal",
  "main": "dist/index.js",
  "unpkg": "dist/index.umd.js",
  "type": "module",
  "types": "dist/_types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:clean": "rm -rf dist",
    "build:types": "tsc --emitDeclarationOnly",
    "build:source": "rollup --silent --config rollup.config.js",
    "build": "npm run build:clean; npm run build:types; npm run build:source",
    "dev": "rollup --config rollup.config.js --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "web3",
    "crypto",
    "solana",
    "web3modal",
    "walletconnect",
    "web3auth"
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "@walletconnect/solib": "^0.1.9"
  }
}
